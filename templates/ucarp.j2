auto ens33
iface ens33 inet static
   address {{ hostvars[inventory_hostname].ansible_host }}
   gateway {{ GATEWAY }}
   ucarp-vid       1
  
   # Virtual Server IP
   ucarp-vip       {{ HA_IP }}
  
   # Shared Password
   ucarp-password  some_password
  
   # Advertisement skew
   ucarp-advskew   1
  
   # Advertisement base interval
   ucarp-advbase   0
  
   # Master server
{% if inventory_hostname == "squid1" %}
   ucarp-master    yes
{%else %}
   ucarp-master    no
{% endif %}
# Virtual interface 
iface ens33:ucarp inet static
  address {{ HA_IP }}/24
  gateway {{ GATEWAY }}